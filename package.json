{
  "name": "vue-vite-express-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev-client": "vite",
    "dev-server": "nodemon --watch src --ext ts,vue --exec \"ts-node --project tsconfig.node.json src/server/server.ts\"",
    "dev": "concurrently \"npm run dev-client\" \"npm run dev-server\"",
    "lint": "eslint .",
    "format": "prettier --write .",
    "preview": "vite preview",
    "build": "vue-tsc -b && vite build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.0",
    "@eslint/js": "^9.38.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.9.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.7",
    "vue-tsc": "^3.1.0"
  }
}
